<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.nas2skupa.eleventhhour.admin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="hr.nas2skupa.eleventhhour.auth.SignInActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity_">
            <intent-filter>
                <action android:name="hr.nas2skupa.eleventhhour.admin.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ProviderDetailsActivity_"/>
        <activity android:name=".ProviderEditActivity_"/>

        <meta-data
            android:name="app_entry_point"
            android:value="hr.nas2skupa.eleventhhour.admin.MAIN" />
    </application>

</manifest>